doctype html
head
	meta(charset='UTF-8')
	title SCP BJCP Scoresheet
	style
		include ../public/stylesheets/cia_scoresheet.css
body
	#primary_header
		.bjcp_badge
			img(alt='Beer Judge Certification Program' src= images.bjcp_logo)
			img(alt='Cane Island Alers Badge' src= images.club_logo)
		.header_content
			.text_bold.text_italic.text_arial_black.text_larger BEER SCORESHEET
			.text_color_gray.text_center.text_size_70 AHA/BJCP Sanctioned Competition Program
			.text_center.text_size_70 Modified Version
		.aha_badge
			img(alt='Cane Island Ailers Operation Fermentation' src= images.comp_logo)
			img(alt='AHA Program' src= images.aha_logo)
		.header_footer
	#information_row
		.judge_information.text_normal
			.dashed_box.padAll_5
				.row.row_height_25
					.col.col_20
						| Judge Name
					.col
						span.user_input_truncated.display_iblock.lheight_normal.text_center.span_width_165 #{judge.firstname} #{judge.lastname}
				.row.row_height_25
					.col.col_20
						| BJCP ID &amp; Rank
					.col
						span.user_input_truncated.display_iblock.lheight_normal.text_center.span_width_165 #{judge.bjcp_id} #{judge.bjcp_rank}
				.row.row_height_25
					.col.col_20
						br
						| &#x9;&#x9;&#x9;&#x9;&#x9;Email
					.col
						span.user_input_truncated.display_iblock.lheight_normal.text_center.span_width_165 #{judge.email}
			.text_size_85
				.row.text_bold.pbot_5.mtop_5
					| Non-BJCP Qualifications:
				.row.pbot_5
					.col.col_25.text_right
						| Cicerone
					.col
						span.display_iblock.border_solid_1.mleft_5.text_center(style='width: 8px; height: 8px; background-color: white;') #{judge.cicerone_rank ? 'X' : ' '}
					.col.col_20.text_right
						| Rank
					.col.col_40
						span.user_input_truncated.display_iblock.text_center.width_100 #{judge.cicerone_rank ? judge.cicerone_rank : ''}
				.row.pbot_5
					.col.col_25.text_right
						| Pro Brewer
					.col
						span.display_iblock.border_solid_1.mleft_5.text_center(style='width: 8px; height: 8px; background-color: white;') #{judge.pro_brewer_brewery ? 'X' : ' '}
					.col.col_20.text_right
						| Brewery
					.col.col_40
						span.user_input_truncated.display_iblock.text_center.width_100 #{judge.pro_brewer_brewery ? judge.pro_brewer_brewery : ''}
				.row.pbot_5
					.col.col_25.text_right
						| Industry
					.col
						span.display_iblock.border_solid_1.mleft_5.text_center(style='width: 8px; height: 8px; background-color: white;') #{judge.industry_description ? 'X' : ' '}
					.col.col_20.text_right
						| Describe
					.col.col_40
						span.user_input_truncated.display_iblock.text_center.width_100 #{judge.industry_description ? judge.industry_description : ''}
				.row.pbot_5
					.col.col_25.text_right
						| Judging
					.col
						span.display_iblock.border_solid_1.mleft_5.text_center(style='width: 8px; height: 8px; background-color: white;') #{judge.judging_years ? 'X' : ' '}
					.col.col_20.text_right
						| Years
					.col.col_40
						span.user_input_truncated.display_iblock.text_center.width_100 #{judge.judging_years ? judge.judging_years : ''}
		.location_information.text_normal.lheight_large
			.row
				span Flight Location
				span.user_input_truncated.display_iblock.lheight_normal.text_center(style='width:150px; margin-right: 5px;') #{scoresheet.session_location}
				span Date
				span.user_input_truncated.display_iblock.lheight_normal.text_center(style='width:150px;') #{new Date(scoresheet.session_date).toLocaleDateString()}
		.row.clearfix
			.entry_information.text_normal.float_left
				.row
					.col.col_40.float_left
						.row.width_100.row_height_15
							.col
								| Category #
							.col(style='width: 49%')
								span.user_input_truncated.display_iblock.lheight_normal.text_center.width_100 #{scoresheet.category}
						.row.width_100.row_height_15
							.col
								| Sub (a-f)
							.col.float_right(style='width: 49%')
								span.user_input_truncated.display_iblock.lheight_normal.text_center.width_100 #{scoresheet.sub}
					.col.col_40.float_right
						.row.entry_number(style='height: 28px;')
							.part2.text_bold Entry #
							.border_solid_1.float_left.text_center.text_resize_marker(style='width: 87%; height: 100%;line-height: 27px;font-stretch: var(--fontStretch);font-size: var(--fontSize);', data-min-text-size='18') #{scoresheet.entry_number}
				.row.width_100.row_height_15.float_left
					.col
						| Subcategory
					.col(style='width: 73%')
						span.user_input_truncated.display_iblock.lheight_normal.text_center.width_100 #{scoresheet.subcategory}
				.row.width_100.row_height_15.float_left
					.col
						| Special Ingredients
					.col(style='width: 60%')
						span.user_input_truncated.display_iblock.lheight_normal.text_center.width_100 #{scoresheet.special_ingredients}
			.border_solid_1.mleft_15.float_left.text_normal(style='height: 75px; width: 65px; padding: 3px; ')
				div Position in flight
				.row(style='margin-bottom: 3px')
					.col.col_40.text_size_70.text_center
						| Entry
					.col.col_40
						span.border_solid_1.display_iblock.width_100.height_20p.mtop_5.text_center.text_resize_marker(style='width: 35px;line-height: 20px;font-stretch: var(--fontStretch);font-size: var(--fontSize);') #{scoresheet.flight_position}
				.row
					.col.col_40.text_size_70.text_center
						| Of
					.col.col_40
						span.border_solid_1.display_iblock.height_20p.text_center.text_resize_marker(style='width: 35px;line-height: 20px;font-stretch: var(--fontStretch);font-size: var(--fontSize);') #{scoresheet.flight_total}
			.border_solid_1.float_left.text_size_50.mleft_15
				.border_bot_solid_1(style='height: 18px')
					span.display_iblock.text_center.float_left(style='width: 46px;')
						| Advanced to
						span.text_bold MINI-BOS
					span.border_solid_1.display_iblock.float_left.text_center.text_resize_marker(style='width: 12px; height: 90%; border-left: 2px solid black; line-height: 18px;') #{scoresheet.mini_boss_advanced ? 'X' : ' '}
				div.background_lgray
					span.border_solid_1.display_iblock.height_20p.text_resize_marker(style='margin: 1px; width: 93%; text-align: center; line-height: 20px; font-stretch: var(--fontStretch);font-size: var(--fontSize);') #{scoresheet.place > 0 ? ['1st','2nd','3rd'][scoresheet.place-1] : ''}
				.text_bold.text_center
					| PLACE
			.float_left
				span.border_solid_4_gray.display_iblock.text_center(style='height: 45px; width: 55px; margin-left: 14px; padding-top: 7px; font-size:calc(100% + 2vw);') #{scoresheet.consensus_score ? scoresheet.consensus_score : scoresheet.judge_total}
			.float_right.text_center(style='width: 140px; height: 17px; background-color: #5a5a5a; color: white; padding-top: 6px; font-size: 12px; margin-right: 2.5px;') #{scoresheet.consensus_score ? 'CONSENSUS SCORE' : 'JUDGE SCORE'}
			.float_left.text_normal.mtop_5(style='margin-left: 35px; margin-right: 5px')
				| Bottle Inspection
				span.display_iblock.border_solid_2.text_bold.text_center(style='height: 8px; width: 8px; margin-left: 8px;') #{scoresheet.bottle_inspection_check ? 'X' : ' '}
				span.text_bold(style="margin-right: 8px;") OK
				span(style="margin-right: 3px;") Comments:
				span.user_input_truncated.display_iblock(style='width: 200px;') #{scoresheet.bottle_inspection_comment}
	#column_container.text_normal
		//
			Aroma Section
			Color slider range: 8-94
		.aroma_section.ptop_5(style='position: relative; ')
			.flag_window(style='margin-left: 20px;')
				.flag_label
					.left_spacer &nbsp;
					.left_point &nbsp;
					.main_flag Aroma
					.flag_point &nbsp;
			span.display_iblock.text_size_70.text_bold(style='background-color: lightgrey; padding: 1px 5px 2px 5px; position: relative; left: 0; top: 0;') Inappropriate
			.row(style='margin-left: 97px;')
				.col(style='position: relative; width: 16%;')
				span.display_iblock(style='width: 20px; background-color: lightgrey;') &nbsp;
			.row(style='margin-left: 25px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Malt
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.aroma_malt + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0%;') |
						span.display_iblock(style='position: relative; left:7%;') |
						span.display_iblock(style='position: relative; left:44%;') |
						span.display_iblock(style='position: relative; left:91%') |
						span.text_size_40.text_bold(style='position: absolute; left:-13px; bottom: 15px;') None
						span.text_size_50.text_bold(style='position: absolute; left:8px; bottom: 15px;') L
						span.text_size_50.text_bold(style='position: absolute; left:42px; bottom: 15px;') M
						span.text_size_50.text_bold(style='position: absolute; left:87px; bottom: 15px;') H
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 7px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.aroma_malt_inappropriate ? 'X' : ' '}
				.display_iblock(style='margin-left: 10px;')
					span.display_iblock.user_input_truncated(style='width:430px;') #{scoresheet.aroma_malt_comment}
			.row(style='margin-left: 25px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Hops
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.aroma_hops + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0%;') |
						span.display_iblock(style='position: relative; left:7%;') |
						span.display_iblock(style='position: relative; left:44%;') |
						span.display_iblock(style='position: relative; left:91%') |
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 7px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.aroma_hops_inappropriate ? 'X' : ' '}
				.display_iblock(style='margin-left: 10px;')
					span.display_iblock.user_input_truncated(style='width:430px;') #{scoresheet.aroma_hops_comment}
			.row(style='margin-left: 25px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Fermentation
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.aroma_fermentation + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0%;') |
						span.display_iblock(style='position: relative; left:7%;') |
						span.display_iblock(style='position: relative; left:44%;') |
						span.display_iblock(style='position: relative; left:91%') |
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 7px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.aroma_fermentation_inappropriate ? 'X' : ' '}
				.display_iblock(style='margin-left: 10px;')
					span.display_iblock.user_input_truncated(style='width:430px;') #{scoresheet.aroma_fermentation_comment}
			.row(style='margin-left: 25px; height: 13px; padding-top: 7px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Other
				.display_iblock(style='margin-left: 11px;')
					span.display_iblock.user_input_truncated(style='width:562px;') #{scoresheet.aroma_other_comment}
			.total.display_iblock.border_solid_1(style='height: 50px; width: 50px; position: absolute; top: 50px; right: 5px; ')
				.text_center(style='height: 64%; padding-top: 7px; width: 100%; background-color: lightgrey; font-size:calc(100% + 2vw);') #{scoresheet.aroma_score}
				.text_right
					span.display_iblock(style='border-left: 1px solid black; border-top: 1px solid black; width: 12px;')
						| 12
		//
			Appearance Section
		.appearance_section.ptop_5(style='position: relative; ')
			.flag_window(style='margin-left: 20px;')
				.flag_label
					.left_spacer &nbsp;
					.left_point &nbsp;
					.main_flag Appearance
					.flag_point &nbsp;
			span.display_iblock.text_size_70.text_bold(style='background-color: lightgrey; padding: 1px 5px 2px 5px; position: relative; right: 35px; top: 0;') Inappropriate
			span.display_iblock.text_size_70.text_bold(style='background-color: lightgrey; padding: 1px 5px 2px 5px; position: relative; left: 210px; top: 0;') Inappropriate
			div(style='width: 20px; background-color: lightgrey; position: relative; bottom: 0; left: 27%;') &nbsp;
			.row(style='margin-left: 25px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Color
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.appearance_color + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0%;') |
						span.display_iblock(style='position: relative; left:17%;') |
						span.display_iblock(style='position: relative; left:35%;') |
						span.display_iblock(style='position: relative; left:52%;') |
						span.display_iblock(style='position: relative; left:69%;') |
						span.display_iblock(style='position: relative; left:86%;') |
						span.text_size_50.text_bold.vertText(style='position: absolute; left:-3px; bottom: 8px;') Yellow
						span.text_size_50.text_bold.vertText(style='position: absolute; left:14px; bottom: 8px;') Gold
						span.text_size_50.text_bold.vertText(style='position: absolute; left:32px; bottom: 8px;') Amber
						span.text_size_50.text_bold.vertText(style='position: absolute; left:50px; bottom: 8px;') Copper
						span.text_size_50.text_bold.vertText(style='position: absolute; left:67px; bottom: 8px;') Brown
						span.text_size_50.text_bold.vertText(style='position: absolute; left:85px; bottom: 8px;') Black
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 7px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.appearance_color_inappropriate ? 'X' : ' '}
				.display_iblock(style='margin-left: 10px;')
					span.display_iblock.user_input_truncated(style='width:125px;') #{scoresheet.appearance_color_other}
				span.text_bold.text_right.display_iblock(style='width: 30px;') Head
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.appearance_head + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0%;') |
						span.display_iblock(style='position: relative; left:22%;') |
						span.display_iblock(style='position: relative; left:44%;') |
						span.display_iblock(style='position: relative; left:66%;') |
						span.display_iblock(style='position: relative; left:88%;') |
						span.display_iblock.text_size_50.text_bold.vertText(style='position: absolute; left:-3px; bottom: 8px;') White
						span.display_iblock.text_size_50.text_bold.vertText(style='position: absolute; left:19px; bottom: 8px;') Ivory
						span.display_iblock.text_size_50.text_bold.vertText(style='position: absolute; left:41px; bottom: 8px;') Beige
						span.display_iblock.text_size_50.text_bold.vertText(style='position: absolute; left:62px; bottom: 8px;') Tan
						span.display_iblock.text_size_50.text_bold.vertText(style='position: absolute; left:84px; bottom: 8px;') Brown
				.display_iblock(style='width: 20px; background-color: lightgrey; position: absolute; top: 37px; right: 229px;') &nbsp;
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 7px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.appearance_head_inappropriate ? 'X' : ' '}
				.display_iblock(style='margin-left: 10px;')
					span.display_iblock.user_input_truncated(style='width:125px;') #{scoresheet.appearance_head_other}
			.row(style='margin-left: 25px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Clarity
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.appearance_clarity + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0px;') |
						span.display_iblock(style='position: relative; left:30%;') |
						span.display_iblock(style='position: relative; left:60%;') |
						span.display_iblock(style='position: relative; left:91%;') |
						span.display_iblock.text_size_50.text_bold(style='position: absolute; left:0px; bottom: 8px;') Brilliant
						span.display_iblock.text_size_50.text_bold(style='position: absolute; left:38px; bottom: 8px;') Hazy
						span.display_iblock.text_size_50.text_bold(style='position: absolute; left:67px; bottom: 8px;') Opaque
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 8.5px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.appearance_clarity_inappropriate ? 'X' : ' '}
				.display_iblock.text_size_40(style='margin-left: 10px; width:111px; position: relative; bottom: 13px; ') Other
				span.text_bold.text_right.display_iblock(style='width: 44px;') Retention
				.slider_parent.display_iblock(style='position: relative; line-height: 8px; z-index: 99; ')
					.sliver_mainline.border_bot_solid_1.display_iblock(style='width: 89px; margin-left: 10px; position: relative; z-index: 100;')
						span.display_iblock(style='width: 8px; height: 8px; background-color: black; border-radius: 50%; position: absolute; z-index: 2000; left:calc(' + scoresheet.appearance_retention + '% - 4px); top: 4px;') &nbsp;
						span.display_iblock(style='position: relative; left:0%;') |
						span.display_iblock(style='position: relative; left:95%; ') |
						span.display_iblock.text_size_50.text_bold(style='position: absolute; left:9px; bottom: 9px;') Quick
						span.display_iblock.text_size_50.text_bold(style='position: absolute; left:65px; bottom: 9px;') Lasting
				.display_iblock(style='width: 20px; background-color: lightgrey; position: absolute; top: 34px; right: 229px;') &nbsp;
				.display_iblock(style='width: 15px; background-color: lightgrey; margin-left: 8.5px; padding-left: 5px; height: 13px; padding-top: 8.5px;')
					span.display_iblock.border_solid_1.text_center(style='width: 8px; height: 8px; background-color: white;') #{scoresheet.appearance_retention_inappropriate ? 'X' : ' '}
				.display_iblock.text_size_40(style='margin-left: 10px; width:111px; position: relative; bottom: 13px; ') Other
			.row(style='margin-left: 25px; height: 13px; padding-top: 7px;')
				span.text_bold.text_right.display_iblock(style='width: 68px;') Other
				.display_iblock(style='margin-left: 11px;')
					span.display_iblock.user_input_truncated(style='width:253px;') #{scoresheet.appearance_other_comment}
				span.text_bold.text_right.display_iblock(style='width: 31px;') Texture
				.display_iblock(style='margin-left: 11px;')
					span.display_iblock.user_input_truncated(style='width:259px;') #{scoresheet.appearance_texture_comment}
			.total.display_iblock.border_solid_1(style='height: 50px; width: 50px; position: absolute; top: 50px; right: 5px; ')
				.text_center(style='height: 64%; padding-top: 7px; width: 100%; background-color: lightgrey; font-size:calc(100% + 2vw);') #{scoresheet.appearance_score}
				.text_right
					span.display_iblock(style='border-left: 1px solid black; border-top: 1px solid black; width: 12px;')
						| 3
		//
			Flavor Section
		.flavor_section.ptop_5(style='position: relative; ')
			.flag_window(style='margin-left: 20px;')
